              [ { "title": "Check if the page is being crawled by a bot",
                  "body":
                    [ "import wixSeo from 'wix-seo';",
                      "",
                      "$w.onReady( () => {",
                      "  if(wixSeo.isInSEO()) {",
                      "     // do something for a bot",
                      "  }",
