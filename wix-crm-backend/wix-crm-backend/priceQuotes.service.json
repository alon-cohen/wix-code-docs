        "docs":
          { "summary": "Updates an existing price quote.",
            "description":
              [ "The `updatePriceQuote()` function returns a Promise that resolves to the updated price quote",
                " ID and version when the price quote is updated." ],
            "links": [],
            "examples":
              [ { "title": "Update an existing price quote",
                  "body":
                    [ "import wixCrmBackend from 'wix-crm-backend';",
                      "",
                      "export function updatePriceQuote(id) {",
                      "  return wixCrmBackend.priceQuotes.getPriceQuote(id)",
                      "  .then( (result) => {",
                      "    let updateRequest = {",
                      "      \"id\": {",
                      "        \"id\": result.id.id,",
                      "        \"version\": result.id.version",
                      "      },",
                      "      \"data\": {",
                      "        \"title\": \"Updated title\",",
                      "        \"customer\": result.customer,",
                      "        \"currency\": result.currency,",
                      "        \"lineItems\": result.lineItems,",
                      "        \"discount\": {},",
                      "        \"paymentTerms\": result.paymentTerms,",
                      "        \"metadata\": result.metadata,",
                      "        \"dates\": result.dates",
                      "      }",
                      "    };",
                      "    return wixCrmBackend.priceQuotes.updatePriceQuote(updateRequest);",
                      "  } );",
                      "}" ],
                  "extra":
                    {  } } ],
            "extra":
              {  } },
