                      "  company: \"Some Company\",",
                      "  fullName: \"John Doe\",",
                      "  firstName: \"John\",",
                      "  lastName: \"Doe\"",
                      "};",
                      "",
                      "let lineItems = [",
                      "  {",
                      "    id: \"00001\",",
                      "    name: \"Item 1\",",
