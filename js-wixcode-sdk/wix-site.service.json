                      "*    \t  \"url\": \"/product-page\",",
                      "*    \t  \"applicationId\": \"1380b703-ce81-ff05-f115-39571d94dfcd\"",
                      "*  \t },",
                      "*    {",
                      "*    \t  \"name\": \"Thank You Page\",",
                      "*    \t  \"type\": \"template\",",
                      "*    \t  \"url\": \"/thank-you-page\",",
                      "*    \t  \"applicationId\": \"1380b703-ce81-ff05-f115-39571d94dfcd\"",
                      "*  \t },",
                      "*  \t {",
                      "*    \t  \"name\": \"Cart\",",
                      "*    \t  \"type\": \"template\",",
