                      "*    \t  \"type\": \"template\",",
                      "*    \t  \"prefix\": \"MyCollection\"",
                      "*  \t },",
                      "*  \t {",
                      "*    \t  \"name\": \"Dashboard Page\",",
                      "*    \t  \"type\": \"static\"",
                      "*    },",
                      "*  \t {",
                      "*    \t  \"name\": \"Shop\",",
                      "*    \t  \"type\": \"template\",",
                      "*    \t  \"url\": \"/shop\",",
