                      "*    \t  \"type\": \"static\",",
                      "*    \t  \"url\": \"/child-page\"",
                      "*  \t },",
                      "*  \t {",
                      "*    \t  \"name\": \"router-page\",",
                      "*    \t  \"type\": \"template\",",
                      "*    \t  \"prefix\": \"router\"",
                      "*  \t },",
                      "*  \t {",
                      "*    \t  \"name\": \"MyCollection (Title)\",",
