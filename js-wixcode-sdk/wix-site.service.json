                      "*  \t }",
                      "* ],",
                      "*   \"prefixes\": [",
                      "*  \t {",
                      "*       \"name\": \"router\",",
                      "*    \t  \"type\": \"router\",",
                      "*    \t  \"prefix\": \"/router\"",
                      "*  \t },",
                      "*  \t {",
                      "*    \t  \"name\": \"MyCollection\",",
