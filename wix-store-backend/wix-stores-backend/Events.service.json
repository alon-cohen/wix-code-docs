    [ { "name": "onCartAbandoned",
        "labels": [],
        "nameParams": [],
        "params":
          [ { "name": "event",
              "type": "wix-stores-backend.Events.CartAbandonedEvent",
              "doc": "Information about the cart that was abandoned." } ],
